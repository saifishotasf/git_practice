<project name="My ant build process" default="build">
  <target name="clean">
    <delete dir="C:\Users\User\Desktop\mycode\build"/>
  </target>
  <target name="init" depends="clean">
    <mkdir dir="C:\Users\User\Desktop\mycode\build"/>
    <mkdir dir="C:\Users\User\Desktop\mycode\build\classes"/>
    <mkdir dir="C:\Users\User\Desktop\mycode\build\artifact"/>
  </target>
  <target name="compile" depends="init">
    <javac srcdir="C:\Users\User\Desktop\mycode\src"
      destdir="C:\Users\User\Desktop\mycode\build\classes"/>
